@model CourseRegistration.Models.CompanyHR

    <div class="form-horizontal">
        <h4>CompanyHR</h4>
        <hr />
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        @Html.HiddenFor(model => model.Email)

